      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "woothee": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^11.11.6",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6",
    "vue": "^2.6.10",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  }
}
