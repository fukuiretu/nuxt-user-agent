{
  "name": "nuxt-user-agent",
  "version": "1.2.2",
  "description": "Nuxt.js module for handling User-Agent.",
  "keywords": [
    "Nuxt.js",
    "nuxt",
    "module",
    "UserAgent"
  ],
  "main": "lib/module.js",
  "types": "lib/types.d.ts",
  "repository": "https://github.com/fukuiretu/nuxt-user-agent",
  "author": "fukuiretu",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "tslint src/*.ts",
    "lint:fix": "tslint src/*.ts --fix",
    "prepublish": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "woothee": "^1.8.1"
  },
  "devDependencies": {
    "@nuxt/types": "^0.2.11",
    "@types/node": "^11.13.20",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "nuxt": "^2.9.2",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.3"
  }
}
