{
  "name": "nuxt-user-agent",
  "version": "1.2.2",
  "description": "Nuxt.js module for handling User-Agent.",
  "keywords": [
    "Nuxt.js",
    "nuxt",
    "module",
    "UserAgent"
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/fukuiretu/nuxt-user-agent",
  "author": "fukuiretu",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "tslint src/*.ts",
    "lint:fix": "tslint src/*.ts --fix",
    "prepublish": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
